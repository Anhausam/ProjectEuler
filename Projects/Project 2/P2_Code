#Project Euler
#Project 2
#Asher Hausam

print("---PROJECT START---")

sum = 0

seed1 = 1
seed2 = 1
number = 0

while True:
  number = seed1 + seed2
  if(number > 4000000):
    break
  seed1 = seed2
  seed2 = number
  if((number % 2 == 0)):
    sum += number

print("SUM:" + str(sum))

input("---PROJECT END---")
